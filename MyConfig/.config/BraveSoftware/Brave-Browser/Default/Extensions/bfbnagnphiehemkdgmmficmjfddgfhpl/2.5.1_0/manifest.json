{
   "author": "Nenad NovakoviÄ‡",
   "background": {
      "persistent": false,
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/default-128.png",
         "16": "icons/default-16.png",
         "32": "icons/default-32.png",
         "48": "icons/default-48.png"
      },
      "default_popup": "popup.html",
      "default_title": "UltraWideo"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "styles/inject.css" ],
      "js": [ "scripts/inject.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "description": "The cross-browser extension that manipulates video aspect ratio to fit your entire screen.",
   "homepage_url": "https://github.com/dvlden/ultrawideo.git",
   "icons": {
      "128": "icons/default-128.png",
      "16": "icons/default-16.png",
      "32": "icons/default-32.png",
      "48": "icons/default-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "UltraWideo",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.5.1"
}
